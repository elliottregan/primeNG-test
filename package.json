    "@types/jasmine": "~3.10.1",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "eslint": "^7.26.0",
    "jasmine-core": "~3.10.1",
    "karma": "~6.3.14",
