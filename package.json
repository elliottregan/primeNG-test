{
  "name": "prime-ng-test",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 4200",
    "start:analyze": "npm run build:stats && npm run analyze",
    "build": "ng build",
    "build:stats": "ng build --stats-json",
    "watch": "ng build --watch --configuration development",
    "analyze": "webpack-bundle-analyzer dist/primeNG-test/stats.json --host 0.0.0.0 --port 4200",
    "test": "ng test",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
